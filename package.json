{
  "name": "@agorize/agorize-uikit",
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "refactor",
              "scope": "core",
              "release": "minor"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agorize/agorize-uikit.git"
  },
  "scripts": {
    "build": "rollup -c",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "test": "jest",
    "notify": "node .circleci/slack.js "
  },
  "version": "0.0.0-development",
  "author": "",
  "keywords": [],
  "license": "ISC",
  "main": "dist/index.min.js",
  "bugs": {
    "url": "https://github.com/Agorize/agorize-uikit/issues"
  },
  "bundleDependencies": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "deprecated": false,
  "description": "",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/github": "^5.2.0",
    "@semantic-release/npm": "^5.0.5",
    "@semantic-release/release-notes-generator": "^7.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "autoprefixer": "^9.2.1",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.0.4",
    "css-loader": "^1.0.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^4.7.1",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.2.0",
    "jest": "^23.6.0",
    "jest-transform-css": "^2.0.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.4",
    "postcss": "^7.0.5",
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.10.5",
    "style-loader": "^0.23.1",
    "vue": "^2.5.17",
    "vue-jest": "^3.0.0",
    "vue-loader": "^15.4.2",
    "vue-styleguidist": "^1.8.9",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2"
  },
  "homepage": "https://github.com/Agorize/agorize-uikit#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "vue2-transitions": "^0.2.3",
    "vuedraggable": "^2.16.0"
  }
}
