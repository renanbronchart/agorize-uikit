{
  "name": "@agorize/gommette",
  "main": "dist/gommette.common.js",
  "module": "dist/gommette.esm.js",
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "parserOpts": {
            "headerPattern": "^(?:(?:ORPHAN|PROD-\\d+|TS-\\d+) )(\\w*)(?:\\(([\\w\\$\\.\\-\\* ]*)\\))?\\: (.*)$"
          },
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "refactor",
              "scope": "core",
              "release": "minor"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agorize/gommette.git"
  },
  "scripts": {
    "build": "rollup -c build/rollup.conf.js",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "test": "jest",
    "notify": "node .circleci/slack.js",
    "gen": "hygen"
  },
  "version": "0.0.0-development",
  "author": "",
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Agorize/gommette/issues"
  },
  "bundleDependencies": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "deprecated": false,
  "description": "",
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@semantic-release/commit-analyzer": "6.1.0",
    "@semantic-release/github": "5.2.5",
    "@semantic-release/npm": "5.1.1",
    "@semantic-release/release-notes-generator": "7.1.4",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/server-test-utils": "1.0.0-beta.27",
    "@vue/test-utils": "1.0.0-beta.27",
    "autoprefixer": "9.4.2",
    "axios": "0.18.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babelrc-rollup": "3.0.0",
    "bootstrap-sass": "3.4.0",
    "commitizen": "3.0.5",
    "css-loader": "1.0.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.10.0",
    "eslint-plugin-vue": "4.7.1",
    "file-loader": "2.0.0",
    "fs": "0.0.1-security",
    "fs-extra": "7.0.1",
    "hygen": "1.6.2",
    "inquirer": "6.2.0",
    "jest": "23.6.0",
    "jest-axe": "3.1.0",
    "jest-transform-css": "2.0.0",
    "lodash": "4.17.11",
    "node-sass": "4.11.0",
    "path": "0.12.7",
    "postcss": "7.0.6",
    "rollup": "0.66.6",
    "rollup-plugin-alias": "1.5.1",
    "rollup-plugin-babel": "3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-eslint": "5.0.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-scss": "0.4.0",
    "rollup-plugin-vue": "4.3.2",
    "sass-loader": "7.1.0",
    "sass-resources-loader": "2.0.0",
    "semantic-release": "15.12.5",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "vue": "2.6.10",
    "vue-jest": "3.0.2",
    "vue-loader": "15.4.2",
    "vue-server-renderer": "2.6.10",
    "vue-styleguidist": "1.9.0",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.27.1"
  },
  "homepage": "https://github.com/Agorize/gommette#readme",
  "jest": {
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "vee-validate": "2.2.0",
    "vue2-transitions": "0.2.3",
    "vuedraggable": "2.17.0"
  }
}
